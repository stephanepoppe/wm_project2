	<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8">
	    <title>Swappy</title>

	    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1" />

	    <!-- Loading Bootstrap -->
	    <link href="{{ app.request.baseUrl }}/css/bootstrap.css" rel="stylesheet">
	    <link href="{{ app.request.baseUrl }}/css/bootstrap-responsive.css" rel="stylesheet">

	    <!-- Loading Flat UI -->
	    <link href="{{ app.request.baseUrl }}/flatui/css/flat-ui.css" rel="stylesheet">

	    <!-- Pop up -->
	    <link href="{{ app.request.baseUrl }}/js/Magnific-Popup/dist/magnific-popup.css" rel="stylesheet">


	    <!-- Style css -->
	    <link href="{{ app.request.baseUrl }}/css/style.css" rel="stylesheet">
	    <link href="http://code.jquery.com/ui/1.10.3/themes/smoothness/jquery-ui.css" rel="stylesheet">

	    <!-- Google fonts -->
	   	<link href='http://fonts.googleapis.com/css?family=Roboto+Slab' rel='stylesheet' type='text/css'>

	</head>

	<body>
		<div class="container">
			<header>
				{% include 'header.twig' %}
			</header>
			<div class="row">
				<div id="alerts" class="span12">
				</div>
			</div>
			<div class="row">
				<div class="span12">
					{% block content %}
    				{% endblock %}
    			</div>
    		</div>
		</div>
	</body>


	<script src="http://code.jquery.com/jquery-1.9.1.min.js"></script>
	<script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
	<script type="text/javascript" src="{{app.request.baseUrl}}/js/bootstrap.min.js"></script>
	<script src="{{ app.request.baseUrl }}/js/Magnific-Popup/dist/jquery.magnific-popup.min.js"></script>
	<script src="{{ app.request.baseUrl }}/js/responsive.js"></script>
	<script src="{{ app.request.baseUrl }}/js/message_polling.js"></script>

	{% block jqueryblock %}
    {% endblock %}

</html>
